    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jasmine": "^5.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
